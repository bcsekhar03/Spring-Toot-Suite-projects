server:
  port: 8081

spring:
  application:
    name: welcome-API
  boot:
    admin:
      client:
        url: http://localhost:8080/
        

eureka:
  client:
    service-url:
      defaultZone: http://localhost:8761/eureka/
    register-with-eureka: true
    fetch-registry: true
    
management:
  endpoints:
    web:
      exposure:
        include: '*'
    
    
